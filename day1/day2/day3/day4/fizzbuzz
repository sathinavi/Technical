#include <stdio.h>

int main()
{
    int n;
    printf("Enter the number: ");
    scanf("%d", &n);  
    for (int i = 0; i <= n; i++)
    {
        if (i % 3 == 0 && i % 5 == 0)
        {
            printf("%d is fizz buzz\n", i);
        }
        else if (i % 3 == 0)
        {
            printf("%d is fizz\n", i);
        }
        else if (i % 5 == 0)
        {
            printf("%d is buzz\n", i);
        }
        else
        {
            printf("%d\n", i);
        }
    }

    return 0;
}
